{
  "model_name": "background-removal",
  "version": "1.0.0",
  "framework": "tensorflow",
  "architecture": "deeplabv3plus",
  "backbone": "mobilenetv2",
  "input_shape": [1, 512, 512, 3],
  "output_shape": [1, 512, 512, 1],
  "preprocessing": {
    "resize": [512, 512],
    "normalize": true,
    "mean": [0.485, 0.456, 0.406],
    "std": [0.229, 0.224, 0.225]
  },
  "inference": {
    "batch_size": 1,
    "precision": "float16",
    "device": "cuda",
    "optimization": "tensorrt"
  },
  "performance": {
    "target_fps": 60,
    "max_latency_ms": 16,
    "min_iou": 0.98
  },
  "model_files": {
    "full_model": "model.h5",
    "weights": "weights.h5",
    "architecture": "architecture.json",
    "onnx": "model.onnx",
    "tflite": "model.tflite",
    "tensorrt": "model.trt"
  },
  "mobile_variants": {
    "ios": {
      "format": "coreml",
      "file": "model.mlmodel",
      "size_mb": 17,
      "target_fps": 30
    },
    "android": {
      "format": "tflite",
      "file": "model_quantized_int8.tflite",
      "size_mb": 12,
      "target_fps": 30
    }
  },
  "dependencies": {
    "tensorflow": ">=2.10.0",
    "opencv-python": ">=4.7.0",
    "numpy": ">=1.23.0",
    "pillow": ">=9.0.0"
  },
  "training": {
    "base_dataset": "coco2017",
    "custom_dataset": "proprietary-video-segmentation",
    "training_samples": 118000,
    "validation_samples": 5000,
    "test_samples": 5000,
    "epochs": 50,
    "optimizer": "adam",
    "learning_rate": 0.0001,
    "loss_function": "binary_crossentropy",
    "metrics": ["iou", "dice_coefficient", "pixel_accuracy"]
  },
  "postprocessing": {
    "edge_refinement": true,
    "morphological_operations": {
      "enabled": true,
      "kernel_size": 5,
      "iterations": 2
    },
    "temporal_smoothing": {
      "enabled": true,
      "window_size": 5,
      "alpha": 0.7
    }
  },
  "hyperparameters": {
    "segmentation_threshold": 0.5,
    "edge_softness": 0.1,
    "feathering_radius": 3,
    "temporal_consistency": 0.8
  },
  "license": "proprietary",
  "created_at": "2024-01-15T00:00:00Z",
  "updated_at": "2024-01-15T00:00:00Z",
  "author": "TelePrompt Pro ML Team"
}
