{
  "name": "teleprompt-pro-backend",
  "version": "1.0.0",
  "description": "Backend API for TelePrompt Pro",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "test": "jest",
    "lint": "eslint src/",
    "dev:ts": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "dev:api-gateway": "nodemon --exec ts-node --transpile-only -P tsconfig.api-gateway.json api-gateway/src/server.ts",
    "build:api-gateway": "tsc -p tsconfig.api-gateway.json",
    "start:api-gateway": "node dist/api-gateway/src/server.js"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.873.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^8.1.5",
    "@tensorflow/tfjs-node": "^4.22.0",
    "apple-signin-auth": "^1.7.6",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^9.10.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.13",
    "openai": "^5.15.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "stripe": "^14.25.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.11",
    "@types/stripe": "^8.0.416",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^5.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
